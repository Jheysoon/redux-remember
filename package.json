{
  "name": "redux-remember",
  "version": "1.0.10",
  "description": "Saves and loads your redux state from a key-value store of your choice",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "browser": "dist/redux-remember.js",
  "scripts": {
    "clean": "rimraf es lib dist 2> /dev/null",
    "build:lib": "cross-env BABEL_ENV=lib babel src --source-maps --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --source-maps --out-dir es",
    "build:umd": "cross-env BABEL_ENV=umd rollup -f umd -c ./rollup.config.js",
    "build": "npm run build:lib && npm run build:es && npm run build:umd",
    "prepare": "npm run clean && npm run build",
    "test": "jest src/__tests__/*.test.js"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./src/__tests__/setup.js",
    "collectCoverage": true,
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "author": "Iskren Slavov <iskren.s@gmail.com>",
  "license": "MIT",
  "files": [
    "dist",
    "es",
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zewish/redux-remember.git"
  },
  "bugs": {
    "url": "https://github.com/zewish/redux-remember/issues"
  },
  "homepage": "https://github.com/zewish/redux-remember/",
  "keywords": [
    "redux",
    "remember",
    "storage",
    "persistance",
    "rehydration"
  ],
  "dependencies": {
    "lodash.isequal": "^4.5.0",
    "lodash.pick": "^4.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.0.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "cross-env": "^5.1.6",
    "jest": "^23.1.0",
    "mocha": "^5.2.0",
    "redux": "*",
    "rimraf": "^2.6.2",
    "rollup": "^0.59.4",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "sinon": "^6.0.0",
    "sinon-chai": "^3.2.0"
  },
  "peerDependencies": {
    "redux": "*"
  }
}

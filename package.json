{
  "name": "redux-remember",
  "version": "1.0.0",
  "description": "Saves and loads your redux state from a key-value store of your choice",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "browser": "dist/redux-remember.js",
  "scripts": {
    "clean": "rimraf es lib dist 2> /dev/null",
    "build:lib": "cross-env BABEL_ENV=lib babel src --source-maps --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --source-maps --out-dir es",
    "build:umd": "cross-env BABEL_ENV=umd rollup -f umd -c ./rollup.config.js",
    "build": "npm run build:lib && npm run build:es && npm run build:umd",
    "prepublish": "npm run clean && npm run build"
  },
  "author": "Iskren Slavov <iskren.s@gmail.com>",
  "license": "MIT",
  "files": [
    "dist",
    "es",
    "lib",
    "src"
  ],
  "dependencies": {
    "lodash.isequal": "^4.5.0",
    "lodash.pick": "^4.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.1.6",
    "rimraf": "^2.6.2",
    "rollup": "^0.59.4",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0"
  }
}

{
  "name": "redux-remember",
  "version": "2.0.3",
  "description": "Saves and loads your redux state from a key-value store of your choice",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "browser": "dist/redux-remember.js",
  "scripts": {
    "clean": "rimraf es lib dist 2> /dev/null",
    "build:lib": "cross-env BABEL_ENV=lib babel src --source-maps --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --source-maps --out-dir es",
    "build:umd": "cross-env BABEL_ENV=umd rollup -f umd -c ./rollup.config.js",
    "build": "npm run build:lib && npm run build:es && npm run build:umd",
    "prepare": "npm run clean && npm run build && npm run test",
    "test": "jest src/__tests__/*.test.js",
    "coveralls": "jest src/__tests__/*.test.js --coverage --coverageReporters=text-lcov | coveralls"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./src/__tests__/setup.js"
    ],
    "collectCoverage": true,
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "author": "Iskren Slavov <iskren.s@gmail.com>",
  "license": "MIT",
  "files": [
    "dist",
    "es",
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zewish/redux-remember.git"
  },
  "bugs": {
    "url": "https://github.com/zewish/redux-remember/issues"
  },
  "homepage": "https://github.com/zewish/redux-remember/",
  "keywords": [
    "redux",
    "remember",
    "storage",
    "persist",
    "persistance",
    "rehydrate",
    "rehydration",
    "localstorage",
    "sessionstorage",
    "asyncstorage",
    "react",
    "react-native"
  ],
  "dependencies": {
    "lodash.isequal": "^4.5.0",
    "lodash.pick": "^4.4.0",
    "lodash.throttle": "^4.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "babel-jest": "^24.8.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "chai": "^4.2.0",
    "coveralls": "^3.0.4",
    "cross-env": "^5.2.0",
    "jest": "^24.8.0",
    "redux": "*",
    "rimraf": "^2.6.3",
    "rollup": "^1.16.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0"
  },
  "peerDependencies": {
    "redux": "*"
  }
}

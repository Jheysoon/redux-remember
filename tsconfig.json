{
  "compilerOptions": {
    "target": "es2022",
    "module": "node16",
    "moduleResolution": "node",
    "lib": ["dom"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "noImplicitAny": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true
  },
  "include": ["src"]
}
